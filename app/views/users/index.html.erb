<% provide(:title, 'All users') %>
<h1>All users</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_with url: search_path, method: 'get', id: 'form', local: true do |f| %>
      <%= f.text_field :name, class: 'form-control' %>
      <%= f.submit "Search Account", class: "btn btn-black" %>
    <% end %>
  </div>
</div>

<ul class="users">
  <%= render @users %>
</ul>
<%= paginate @users %>