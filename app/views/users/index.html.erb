<% provide(:title, 'All users') %>
<h1>All users</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_tag '/search', :method => 'get' do %>
      <%= text_field_tag :search, params[:search], class: 'form-control' %>

      <%= submit_tag "Search Account", :name=> nil, class: "btn btn-brack" %>
    <% end %>
  </div>
</div>

<ul class="users">
  <%= render @users %>
</ul>
<%= paginate @users %>