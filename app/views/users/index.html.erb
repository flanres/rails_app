<% provide(:title, 'All users') %>
<h1>All users</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%#= form_tag search_path, :method => 'get', id: 'form' do %>
      <%#= text_field_tag :keyword, params[:keyword], class: 'form-control' %>

      <%#= submit_tag "Search", class: "btn btn-brack" %>
    <%= form_with url: search_path, method: 'get', id: 'form', local: true do |f| %>
      <%= f.text_field :input_name, class: 'form-control' %>
      <%= f.submit "Search Account", class: "btn btn-black" %>
    <% end %>
  </div>
</div>

<ul class="users">
  <%= render @users %>
</ul>
<%= paginate @users %>

<ul id="result">
</ul>